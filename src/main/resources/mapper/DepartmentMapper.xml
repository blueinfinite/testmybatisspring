<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.blueinfinite.mapper.DepartmentMapper">
    <select id="getInfo" parameterType="int" resultType="com.blueinfinite.model.Department">
        select id,name as className from department where id=#{ID}
    </select>
</mapper>